package com.hiklife.svc.console;
/**
 * 键盘key值定义
 */
public class Key {
    public static final int KEY_ESCAPE = 0xff1b; // Escape key
    public static final int KEY_F1 = 0xffbe; // F1 key
    public static final int KEY_F2 = 0xffbf; // F2 key
    public static final int KEY_F3 = 0xffc0; // F3 key
    public static final int KEY_F4 = 0xffc1; // F4 key
    public static final int KEY_F5 = 0xffc2; // F5 key
    public static final int KEY_F6 = 0xffc3; // F6 key
    public static final int KEY_F7 = 0xffc4; // F7 key
    public static final int KEY_F8 = 0xffc5; // F8 key
    public static final int KEY_F9 = 0xffc6; // F9 key
    public static final int KEY_F10 = 0xffc7; // F10 key
    public static final int KEY_F11 = 0xffc8; // F11 key
    public static final int KEY_F12 = 0xffc9; // F12 key
    public static final int KEY_PRINT_SCREEN = 0xff61; // Print Screen key
    public static final int KEY_SCROLL_LOCK = 0xff14; // Scroll Lock key
    public static final int KEY_PAUSE = 0xff13; // Pause key
    public static final int KEY_BACK_QUOTE = 0x0060; // Back Quote key
    public static final int KEY_0 = 0x0030; // 0 key
    public static final int KEY_1 = 0x0031; // 1 key
    public static final int KEY_2 = 0x0032; // 2 key
    public static final int KEY_3 = 0x0033; // 3 key
    public static final int KEY_4 = 0x0034; // 4 key
    public static final int KEY_5 = 0x0035; // 5 key
    public static final int KEY_6 = 0x0036; // 6 key
    public static final int KEY_7 = 0x0037; // 7 key
    public static final int KEY_8 = 0x0038; // 8 key
    public static final int KEY_9 = 0x0039; // 9 key
    public static final int KEY_MINUS = 0x002d; // Minus key
    public static final int KEY_EQUALS = 0x003d; // Equals key
    public static final int KEY_BACK_SPACE = 0xFF08; // Back Space key
    public static final int KEY_TAB = 0xFF09; // Tab key
    public static final int KEY_A = 0x0041; // A key
    public static final int KEY_B = 0x0042; // B key
    public static final int KEY_C = 0x0043; // C key
    public static final int KEY_D = 0x0044; // D key
    public static final int KEY_E = 0x0045; // E key
    public static final int KEY_F = 0x0046; // F key
    public static final int KEY_G = 0x0047; // G key
    public static final int KEY_H = 0x0048; // H key
    public static final int KEY_I = 0x0049; // I key
    public static final int KEY_J = 0x004A; // J key
    public static final int KEY_K = 0x004B; // K key
    public static final int KEY_L = 0x004C; // L key
    public static final int KEY_M = 0x004D; // M key
    public static final int KEY_N = 0x004E; // N key
    public static final int KEY_O = 0x004F; // O key
    public static final int KEY_P = 0x0050; // P key
    public static final int KEY_Q = 0x0051; // Q key
    public static final int KEY_R = 0x0052; // R key
    public static final int KEY_S = 0x0053; // S key
    public static final int KEY_T = 0x0054; // T key
    public static final int KEY_U = 0x0055; // U key
    public static final int KEY_V = 0x0056; // V key
    public static final int KEY_W = 0x0057; // W key
    public static final int KEY_X = 0x0058; // X key
    public static final int KEY_Y = 0x0059; // Y key
    public static final int KEY_Z = 0x005A; // Z key
    public static final int KEY_BRACKET_LEFT = 0x005b; // Left Bracket key
    public static final int KEY_BACK_SLASH = 0x005c; // Back Slash key
    public static final int KEY_BRACKET_RIGHT = 0x005d; // Right Bracket key
    public static final int KEY_CAPS_LOCK = 0xffe5; // Caps Lock key
    public static final int KEY_SEMICOLON = 0x003b; // Semicolon key
    public static final int KEY_COLON = 0x0022; // Colon key
    public static final int KEY_ENTER = 0xFF0D; // Enter key
    public static final int KEY_SHIFT_LEFT = 0xFFE1; // Left Shift key
    public static final int KEY_COMMA = 0x002c; // Comma key
    public static final int KEY_PERIOD = 0x002e; // Period key
    public static final int KEY_SLASH = 0x002f; // Slash key
    public static final int KEY_SHIFT_RIGHT = 0xFFE2; // Right Shift key
    public static final int KEY_CTRL_LEFT = 0xFFE3; // Left Control key
    public static final int KEY_WIN_LEFT = 0xffeb; // Left Windows key
    public static final int KEY_ALT_LEFT = 0xFFE9; // Left Alt key
    public static final int KEY_SPACE = 0x0020; // Space key
    public static final int KEY_ALT_RIGHT = 0xFFEA; // Right Alt key
    public static final int KEY_WIN_RIGHT = 0xffec; // Right Windows key
    public static final int KEY_CTRL_RIGHT = 0xFFE4; // Right Control key
    public static final int KEY_INSERT = 0xFF63; // Insert key
    public static final int KEY_HOME = 0xFF50; // Home key
    public static final int KEY_PAGE_DOWN = 0xFF56; // Page Down key
    public static final int KEY_PAGE_UP = 0xFF55; // Page Up key
    public static final int KEY_DELETE = 0xFFFF; // Delete key
    public static final int KEY_END = 0xFF57; // End key
    public static final int KEY_DOWN = 0xFF54; // Down Arrow key
    public static final int KEY_RIGHT = 0xFF53; // Right Arrow key
    public static final int KEY_UP = 0xFF52; // Up Arrow key
    public static final int KEY_LEFT = 0xFF51; // Left Arrow key
    public static final int KEY_NUM_LOCK = 0xff7f; // Num Lock key
    public static final int KEY_NUMPAD0 = 0xffb0; // Numpad 0 key
    public static final int KEY_NUMPAD1 = 0xffb1; // Numpad 1 key
    public static final int KEY_NUMPAD2 = 0xffb2; // Numpad 2 key
    public static final int KEY_NUMPAD3 = 0xffb3; // Numpad 3 key
    public static final int KEY_NUMPAD4 = 0xffb4; // Numpad 4 key
    public static final int KEY_NUMPAD5 = 0xffb5; // Numpad 5 key
    public static final int KEY_NUMPAD6 = 0xffb6; // Numpad 6 key
    public static final int KEY_NUMPAD7 = 0xffb7; // Numpad 7 key
    public static final int KEY_NUMPAD8 = 0xffb8; // Numpad 8 key
    public static final int KEY_NUMPAD9 = 0xffb9; // Numpad 9 key
    public static final int KEY_NUMPAD_MULTIPLY = 0xffaa; // Numpad Multiply key
    public static final int KEY_NUMPAD_ADD = 0xffab; // Numpad Add key
    public static final int KEY_NUMPAD_DIVIDE = 0xffaf; // Numpad Divide key
    public static final int KEY_NUMPAD_SUBTRACT = 0xffad; // Numpad Subtract key
    public static final int KEY_NUMPAD_DECIMAL = 0xffae; // Numpad Decimal key
}